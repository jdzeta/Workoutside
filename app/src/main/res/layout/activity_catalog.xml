<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.ecolem.workoutside.activities.CatalogActivity">

    <!--TextView android:text="@string/title_activity_catalogue"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/title_catalogue"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/catalogue_nom"
        android:layout_marginTop="20dp"
        android:hint="NomCatalogue"
        android:layout_below="@+id/title_catalogue"/>

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/catalogue_image"
        android:layout_marginTop="20dp"
        android:hint="ImageCatalogue"
        android:layout_below="@+id/catalogue_nom"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/catalogue_description"
        android:layout_marginTop="20dp"
        android:hint="DescriptionCatalogue"
        android:layout_below="@+id/catalogue_image"/>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Mouvements"
        android:id="@+id/btn_mouvements"
        android:layout_below="@+id/catalogue_description"
        android:layout_marginTop="20dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" /-->


    <ListView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/catalog_listview"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true" />
</RelativeLayout>
