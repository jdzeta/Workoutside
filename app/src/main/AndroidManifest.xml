<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ecolem.workoutside" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".activities.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activities.CompteActivity"
            android:label="@string/title_activity_compte" >
        </activity>
        <activity
            android:name=".activities.MouvementActivity"
            android:label="@string/title_activity_mouvement" >
        </activity>
        <activity
            android:name=".activities.NewMouvementActivity"
            android:label="@string/title_activity_new_mouvement" >
        </activity>
        <activity
            android:name=".activities.ListMouvementActivity"
            android:label="@string/title_activity_list_mouvement" >
        </activity>
        <activity
            android:name=".activities.CatalogueActivity"
            android:label="@string/title_activity_catalogue" >
        </activity>
        <activity
            android:name=".activities.ListCatalogueActivity"
            android:label="@string/title_activity_list_catalogue" >
        </activity>
    </application>

</manifest>
